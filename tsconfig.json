{
  "compilerOptions": {
    "target": "es2016",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "commonjs",
    "baseUrl": ".",
    "resolveJsonModule": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "strictPropertyInitialization": false,
    "skipLibCheck": true,
    "paths": {
      "@types/*": ["./src/@types/*"],
      "@prisma/*": ["./prisma/*"],
      "@modules/*": ["./src/modules/*"],
      "@settings/*": ["./src/settings/*"],
      "@shared/*": ["./src/shared/*"],
      "@utils/*": ["./src/utils/*"]
    }
  }
}
